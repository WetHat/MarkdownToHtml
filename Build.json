{
    "markdown_dir": "markdown",

    "site_dir": "docs",

    "HTML_template": "Template",

    "Exclude": [
    ],

    "markdown_extensions": [
        "common",
        "definitionlists",
        "mathematics",
        "diagrams",
        "pipetables",
        "gridtables",
        "autoidentifiers",
        "attributes"
    ],

    "footer": "Site created by PowerShell Module <a href='https://github.com/WetHat/MarkdownToHtml'>MarkdownToHTML</a>.",

    "navigation_bar": {
        "capture_page_headings": "123456",
        "templates": {
            "navitem": "<button class='navitem'><a href='{{navurl}}'>{{navtext}}</a></button>",
            "navlabel": "<div class='navlabel'>{{navtext}}</div>",
            "navseparator": "<hr/>",
            "navheading": "<span class='navitem{{level}}'>{{navtext}}</span>"
        }
    },

    "site_navigation": [
        { "<img width='90%' src='Markdown2HTML.png'/>": "index.md" },
        { "MarkdownToHtml": "" },
        { "Home": "index.md" },
        { "Module Manifest": "{{version}}/MarkdownToHTML.md"},
        { "About MarkdownToHtml": "{{version}}/about_MarkdownToHTML.md" },
        { "Page Content": "" }
    ],

     "github_pages": true,

     "svgbob": {
        "background": "white",
        "fill_color": "black",
        "font_size": 14,
        "font_family": "Monospace",
        "scale": 1,
        "stroke_width": 2
    }
  }